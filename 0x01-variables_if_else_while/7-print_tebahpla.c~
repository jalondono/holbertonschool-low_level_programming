#include <stdio.h>
#include <stdlib.h>
#include <time.h>
/**                                                                                                                                                                           * main - prueba 2                                                                                                                                                            * Return: always 0                                                                                                                                                           */
int main(void)
{
  char i = 'z';
  for (i = 'z'; i >= 'a'; i--)
    {
      putchar (i);
    }
  putchar ('\n');
  return (0);
}
